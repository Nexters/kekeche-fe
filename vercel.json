{
    "headers": [
        {
            "source": "/assets/fonts/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "s-maxage=31536000, immutable"
                }
            ]
        },
        {
            "source": "/manifest.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, s-maxage=86400"
                }
            ]
        },
        {
            "source": "/favicon.ico",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, s-maxage=86400"
                }
            ]
        },
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, s-maxage=86400"
                }
            ]
        }
    ]
}
